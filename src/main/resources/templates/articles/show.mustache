{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">제목</th>
        <th scope="col">문의 내용</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
    {{/article}}
    </tbody>
</table>

{{>comments/_comments}}
<a href="/articles/{{article.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/articles/{{article.id}}/delete" class="btn btn-danger">삭제하기</a>
<a href="/articles">목록</a>

{{>layouts/footer}}